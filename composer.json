{
    "name": "ayctor/wp-core",
    "description": "Core functionalities for ayctor wp template",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Couteillou",
            "email": "damien@ayctor.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php" : "^7.1",
        "cron/cron": "^1.4.1",
        "filp/whoops": "^2.4.1",
        "illuminate/config": "^5.8.27",
        "illuminate/view": "^5.8.27",
        "mikehaertl/phpwkhtmltopdf": "^2.4.0",
        "mmucklo/inflect": "^0.3.0",
        "phpoffice/phpspreadsheet": "^1.8.1",
        "symfony/var-dumper": "^4.3.2",
        "tgmpa/tgm-plugin-activation": "dev-develop",
        "vlucas/phpdotenv": "^3.4.0"
    },
    "autoload": {
        "psr-0": {
            "WpCore": "src/"
        },
        "files": ["src/WpCore/helpers.php"]
    }
}
